{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "ES2019",
    "module": "ESNext",
    "lib": ["ES2020", "DOM"],
    "moduleResolution": "Node",
    "strict": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "jsx": "react",
    "jsxFactory": "createElement"
  }
}
