{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "sourceMap": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "jsxFactory": "createElement",
      "allowJs": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports":true,
    }
  }
