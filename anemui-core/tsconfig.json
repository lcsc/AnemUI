{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "sourceMap": true,
      "target": "es2020",
      "module": "esnext",
      "lib": ["es2020", "dom"],
      "jsx": "react",
      "jsxFactory": "createElement",
      "allowJs": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports":true,
      "types": ["@types/gtag.js"]
    }
